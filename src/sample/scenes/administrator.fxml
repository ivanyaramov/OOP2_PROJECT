<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="514.0" prefWidth="682.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.sceneControllers.AdministratorController">
   <children>
      <Button layoutX="511.0" layoutY="88.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="140.0" text="Change Role" onAction="#changeRole" />
      <ComboBox layoutX="310.0" layoutY="94.0" prefWidth="150.0" promptText="Roles" fx:id="roleFXML">
              <items>
                  <FXCollections fx:factory="observableArrayList">
                      <String fx:value="ADMIN"/>
                      <String fx:value="OWNER"/>
                      <String fx:value="MANAGER"/>
                      <String fx:value="RECEPTIONIST"/>
                      <String fx:value="CLIENT"/>
                  </FXCollections>
              </items>
      </ComboBox>
      <TableView fx:id="tbData" layoutX="25.0" layoutY="22.0" prefHeight="200.0" prefWidth="270.0">
        <columns>
          <TableColumn fx:id="username" prefWidth="100.0" text="Username" />
          <TableColumn fx:id="role" prefWidth="100.0" text="Current role" />
        </columns>
      </TableView>
       <Button layoutX="511.0" layoutY="450.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="170.0" text="Create new Owner" onAction="#createNewOwner" />
       <Button layoutX="511.0" layoutY="500.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="170.0" text="Back" onAction="#redirectBack" />
   </children>
</AnchorPane>
